   {
     "persistent_att_rules":[
    	{
    		"name":"hasPrerequisite",
        "type":"binary",
        "properties": [{"name":"suitability","type":"Boolean","defval":""}],
    		"code":"#suitability:Boolean & `${%target%#knowledge > 0.7}`"
    	},
      {
        "name":"visited",
        "type":"unary",
        "properties":[{"name":"visited", "type":"Integer", "defval":""}],
        "code":"#[visited]:Integer event `${#visited}++;`"
      },
      {
        "name":"own knowledge update",
        "type":"unary",
        "properties":[{"name":"own-knowledge", "type":"Double", "defval":""}],
        "code":"#[own-knowledge]:Double event +`if(${#suitability}) ${#own-knowledge} = 1; else if(${#own-knowledge} < 0.3) ${#own-knowledge} = 0.3;`"
      }
    ],
    "def_att_rules":[
      {
        "name":"knowledge required",
        "type":"binary",
        "target":"suitability",
        "code":"${%target%#knowledge} > 0.7"
      },
      {
        "name":"parent available",
        "type":"unary",
        "target":"suitability",
        "code":"${=>(parent)#availability}"
      },
      {
        "name":"parent subject known",
        "type":"unary",
        "target":"availability",
        "code":"${<=(parent)#knowledge > 0.3}"
      },
      {
        "name":"childAverage",
        "type":"unary",
        "target":"knowledge",
        "code":"avg(new Object[] {${<=(parent)#knowledge}, ${#own-knowledge}})"
      }
    ]
  }
